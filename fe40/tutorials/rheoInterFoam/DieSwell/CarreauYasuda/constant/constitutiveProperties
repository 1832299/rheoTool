                eta0            eta0   [ 1 -1 -1 0 0 0 0 ] 1e6; 
   		etaInf          etaInf  [ 1 -1 -1 0 0 0 0 ] 1e-3; 
   		k               k     [ 0 0 1 0 0 0 0 ] 1.389495494e7; 
   		n               n     [ 0 0 0 0 0 0 0 ] 0.3;   
   		a               a     [ 0 0 0 0 0 0 0 ] 2;
       }
}

phase2
{
     parameters
       {		        
         type            Newtonian;
