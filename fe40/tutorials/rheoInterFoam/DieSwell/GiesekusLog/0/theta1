FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      theta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
   inlet
    {
       type            fixedValue;
       value           uniform (0 0 0 0 0 0);
    }
    
    outlet
    {
       type            zeroGradient;
    }
    
    wallIn
    {
      type             zeroGradient;
    }
    
    wallOut
    {
       type            zeroGradient;
    }
    
    atmosphere
    {
         type          zeroGradient;
    }
    
    symmetry
    {
       type            symmetryPlane;
    }
}
