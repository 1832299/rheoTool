FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      ciCation;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 -3 0 0 1 0 0 ];

internalField   uniform 9.43798e-05;

boundaryField
{
   inletOutlet
    {
	type            cyclic;
    }
    
    symmetry
    {
	type            symmetryPlane;    
    }
    
    walls
    {
	type            boltzmannEquilibrium;
	c0              9.43798e-05;
	psi0            0;
	value           uniform 9.43798e-05; 
    }
    

    frontAndBack
    {
	type            empty;   
    }
}

