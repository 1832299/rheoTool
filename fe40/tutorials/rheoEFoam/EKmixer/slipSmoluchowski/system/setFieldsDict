FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue C 0
);

regions
(
    boxToCell
    {
        box (-1500e-6 0 0) (-50e-6 50e-6 50e-6);
        fieldValues
        (
            volScalarFieldValue C 1
        ); 
    }
);

